[black]
line-length = 0
target-version = ['py37']
include = '\.py?$'
exclude = '''
/(
        \.eggs    # exclude a few common directories in the
    | \.git     # root of the project
    | \.hg
    | \.mypy_cache
    | \.tox
    | \.venv
    | _build
    | buck-out
    | build
    | dist
)/
'''
check = true
diff = true
fast = false
quiet = false
verbose = true
multi-line = 2
max-complexity = 10
